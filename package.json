{
  "name": "backend-test",
  "version": "0.1.0",
  "description": "Backend Test",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "start": "npm run build && npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node --inspect dist/src/server.js",
    "watch-node": "nodemon dist/src/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose --runInBand",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "lint": "npm run tslint"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.0",
    "express-session": "^1.17.0",
    "express-validator": "^4.3.0",
    "extend": ">=3.0.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "lusca": "^1.6.1",
    "mocha": "^8.3.2",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "randexp": "^0.5.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "rmdir": "^1.2.0",
    "sharp": "^0.25.2",
    "tsconfig-paths": "^3.9.0",
    "zip-stream": "^4.0.2"
  },
  "devDependencies": {
    "@types/async": "^2.4.2",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.15",
    "@types/compression": "0.0.33",
    "@types/connect-mongo": "0.0.34",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "0.0.38",
    "@types/express-session": "^1.15.16",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.149",
    "@types/lusca": "^1.6.1",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.22",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.46",
    "@types/sharp": "^0.24.0",
    "@types/shelljs": "^0.7.9",
    "@types/supertest": "^2.0.8",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.6.1",
    "jest": "^26.1.0",
    "nodemon": "^1.19.4",
    "shelljs": "^0.7.7",
    "supertest": "^3.4.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.0.0",
    "typescript": "^3.9.9"
  }
